{
  "dress": {
    "parenthesis": {
      "regex": "\\([^)]+\\)",
      "colors": [
        "red",
        "bright",
        "underline",
        "green_background"
      ]
    },
    "test2": {
      "regex": "mist",
      "skip": true
    },
    "test3": {
      "regex": "(\\d{2})-(\\d{2})",
      "replace": "\\1h:\\2m"
    }
  }
}
